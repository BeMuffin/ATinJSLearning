{
  "name": "atinjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:ui": "npm run build && npm run lint && npx wdio run _dist/wdio.conf.js",
    "wdio": "node _dist/scripts/run-wdio.js",
    "apiJest": "npm run build && jest --config _dist/config/jest.config.js --detectOpenHandles",
    "apiJest:parallel": "jest --config _dist/config/jest.config.js --maxWorkers=5",
    "lint": "eslint --ext .ts src/",
    "build": "tsc -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeMuffin/ATinJSLearning.git"
  },
  "author": "Anna Bichevskaya",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/parser": "^5.61.0",
    "@wdio/cli": "^8.11.4",
    "@wdio/cucumber-framework": "^8.11.3",
    "@wdio/local-runner": "^8.11.3",
    "@wdio/spec-reporter": "^8.11.2",
    "axios": "^1.4.0",
    "chromedriver": "^114.0.2",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.6.1",
    "jest-parallel": "^0.0.0",
    "log4js": "^6.9.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-reportportal-reporter": "^7.6.0",
    "wdio-reportportal-service": "^7.3.1"
  },
  "dependencies": {
    "eslint": "^8.44.0"
  }
}
